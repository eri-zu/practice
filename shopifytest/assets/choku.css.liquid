.btn {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #093057;
  cursor: pointer;
  position: relative;
  overflow: hidden;
}

div[data-csshover="loop"] .btn {
  background-color: #093057 !important;
}

.btn__txt-loop {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  pointer-events: none;
  width: 100%;
}

.btn__txt-loop span {
  font-style: italic;
}

.btn__txt-loopinner {
  width: fit-content;
  display: flex;
  position: relative;
  --offset: 1rem;
  --move-initial: calc(-25% + var(--offset));
  --move-final: calc(-50% + var(--offset));
  transform: translate3d(var(--move-initial), 0, 0);
  animation: loop 1s linear infinite;
  animation-play-state: paused;
  opacity: 0;
}

@keyframes loop {
  0% {
    transform: translate3d(var(--move-initial), 0, 0);
  }
  100% {
    transform: translate3d(var(--move-final), 0, 0);
  }
}

.btn__txt-loopinner span {
  white-space: nowrap;
  text-align: center;
  display: block;
  flex-shrink: 0;
  padding: 0 10rem;
  height: 100%;
  line-height: 80rem;
}

@media only screen and (min-width: 769px) {
  .css-hover[data-csshover="loop"]:hover .css-hover_txt {
    opacity: 0;
  }
  .css-hover[data-csshover="loop"]:hover .css-hover_loop {
    opacity: 1;
    animation-play-state: running;
    transition-duration: 0.4s;
  }
}

.btn__txt {
  font-size: 32rem;
  color: #fff;
  display: inline-block;
  padding-top: 3rem;
}

@media only screen and (max-width: 768px) {
  .btn__txt {
    font-size: 20px;
    letter-spacing: 0.014em;
    padding-top: 3px;
  }
}

.btn__bg {
  position: absolute;
  width: 100%;
  height: 100%;
  display: block;
  pointer-events: none;
  z-index: -1;
}

.btn__bg.js-hover_skew_bg {
  position: absolute;
  width: 130%;
  height: 100%;
  top: 0;
  left: 50%;
  background-color: #093057;
  transform: translateX(-50%) skew(30deg);
  display: block;
  pointer-events: none;
  z-index: -1;
}

.btn__border {
  border: 1px solid #093057;
  position: absolute;
  top: 0;
