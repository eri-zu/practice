<% var rootPATH = './', PATH = './', page = 'top'; %>
<!DOCTYPE html>
<html lang="ja">
<head>
<meta name="viewport" content="width=device-width, initial-scale=1 ,user-scalable=no">
<%- include(rootPATH + 'inc/_style'); %>
<link href="<%- PATH %>assets/css/style.css" rel="stylesheet">
</head>
<body id="<%- page %>" class="">

<div id="wrapper" class="id_<%- page %>">
<div id="inner">
  
  <div class="open open_menu">
    <div class="openWrap">
      <div class="bar"></div>
      <div class="bar"></div>
      <div class="bar"></div>
      <%#- include('../../dist/assets/resource/svg/menu.svg'); %>
    </div>
    <div class="closeWrap">
      <div class="barWrap"><div class="bar"></div></div>
      <div class="barWrap"><div class="bar"></div></div>
    </div>
  </div>

  <div class="close close_menu">
    <div class="barWrap"><div class="bar"></div></div>
    <div class="barWrap"><div class="bar"></div></div>
  </div>

</div>
</div>

<%- include(rootPATH + 'inc/_script', {PATH: PATH}); %>
</body>
</html>